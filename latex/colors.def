% PRINT
% Farbdefinition für graue Hinterlegung etc.
\definecolor{gray050}{cmyk}{0,0,0,0.50}
\definecolor{gray020}{cmyk}{0,0,0,0.20}

% WEB
% Farbdefinitionen
\definecolor{gray}{rgb}{0.4,0.4,0.4}
\definecolor{xmlBlue}{rgb}{0.0,0.0,0.6}
\definecolor{cyan}{rgb}{0.0,0.6,0.6}
\definecolor{xmlOrange}{rgb}{1.0,0.5,0.31}
\definecolor{xmlBrown}{rgb}{0.64,0.25,0.12}
\definecolor{xmlGreen}{rgb}{0.0,0.3,0.1}
\definecolor{mylink}{RGB}{90,90,90}

% Eigener Befehl für Grausatz
\newcommand{\gray}[1]{%
{\color{gray050}#1}%
}
\newcommand{\xmlTagStart}[1]{%
{\texttt{\color{xmlBlue}<#1>}}%
}
\newcommand{\xmlTagEnd}[1]{%
{\texttt{\color{xmlBlue}</#1>}}%
}
\newcommand{\xmlTagEmpty}[1]{%
{\texttt{\color{xmlBlue}<#1/>}}%
}
\newcommand{\xmlAttrKey}[1]{%
{\color{xmlOrange}#1=}%
}
\newcommand{\xmlAttrVal}[1]{%
"{\color{xmlBrown}#1}"%
}
\newcommand{\xml}[1]{
	\texttt{<#1>}
}